/**
 * This class tests a bunch of different method declarations
 */
class Test {
    public blah() => () { ; }   // basic void

    public blah(int a) asdf => () { ; }   // built-in to void
    public blah(code a) => () { ; }   // built-in to void
    public blah(float a) => () { ; }   // built-in to void

    public blah() => (int) { ; }   // void to built-in
    public blah() => (code) { ; }   // void to built-in
    public blah() => (float) { ; }   // void to built-in
    
    public blah(int a) => (int) { ; }   // built-in to built-in
    public blah(float a) => (int) { ; }   // built-in to built-in
    public blah(int a) => (float) { ; }   // built-in to built-in
    
    public blah(int a, int b) => () { ; }
    public blah(int a, int b) => (int) { ; }
    public blah(int a, int b) => (int, int) { ; }
    
    // arrays
    public blah(int[] a) => () { ; }
    public blah(int[] a, float[] b) => () { ; }
    public blah() => (int[]) { ; }
    public blah() => (int[], float[]) { ; }
    public blah() => (int[], int[]) { ; }

    // functions
    public blah(() => () f) => () { ; }
    public blah(int a, () => () f) => () { ; }
    public blah(() => () f, int a) => () { ; }
    public blah(int a, () => () f, int b) => () { ; }
    public blah((int) => () f) => () { ; }
    public blah(() => (int) f) => () { ; }
    public blah((int) => (int) f) => () { ; }
    public blah(int a, (int) => (int) f) => () { ; }
    public blah((int) => (int) f, int a) => () { ; }
    public blah(int a, (int) => (int) f, int a) => () { ; }
    
    public blah() => (() => ()) { ; }
    public blah() => (int, () => ()) { ; }
    public blah() => (() => (), int) { ; }
    public blah() => (int, () => (), int) { ; }
    public blah() => ((int) => ()) { ; }
    public blah() => (() => (int)) { ; }
    public blah() => ((int) => (int)) { ; }
    public blah() => (int, (int) => (int)) { ; }
    public blah() => ((int) => (int), int) { ; }
    public blah() => (int, (int) => (int), int) { ; }
    
    public blah(() => () f) => (() => ()) { ; }
    public blah(int a, () => () f, int a) => (() => ()) { ; }
    public blah(() => () f) => (int, () => (), int) { ; }
    public blah(int a, () => () f, int a) => (int, () => (), int) { ; }
    
    public blah((int) => (int) f) => ((int) => (int)) { ; }
    public blah(int a, (int) => (int) f, int a) => (int, (int) => (int), int) { ; }
    
    // multiple functions
    public blah(() => () f, () => () f) => () { ; }
    public blah(() => () f, () => () f) => (() => ()) { ; }
    public blah(() => () f, () => () f) => (() => (), () => ()) { ; }
    
    public blah((int) => (int) f, (int) => (int) f) => () { ; }
    public blah((int) => (int) f, (int) => (int) f) => ((int) => (int)) { ; }
    public blah((int) => (int) f, (int) => (int) f) => ((int) => (int), (int) => (int)) { ; }
    
    // functions inside of functions
    public blah((()=>()) => () f) => () { ; }
    public blah((()=>()) => (()=>()) f) => () { ; }
    public blah((()=>()) => (()=>()) f) => (()=>()) { ; }
    
    public blah(((int)=>(int)) => () f) => () { ; }
    public blah(((int)=>(int)) => ((int)=>(int)) f) => () { ; }
    public blah(((int)=>(int)) => ((int)=>(int)) f) => ((int)=>(int)) { ; }
}


