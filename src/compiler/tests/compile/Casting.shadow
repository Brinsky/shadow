class Casting {
    // casting for members
    public int a = cast<int>4;
    public int a = cast<int>(4);
    public int a = cast<int>b;
    public int a = cast<int>(b);
    public int a = cast<int>(new int[3][4]);

	//check all types
    public float a = cast<float>(4);
    public double a = cast<double>(4);
    public code a = cast<code>(4);
    public byte a = cast<byte>(4);
    public boolean a = cast<boolean>(false);     
//    public Wombat a = cast<Wombat>(new Object());  
    
    public () => () fun = cast<()=>()>blah;        
    
    public blah() => () {
	    // now in a method
	    int a = cast<int>4;
	    int a = cast<int>(4);
	    int a = cast<int>b;
	    int a = cast<int>(b);
	    int a = cast<int>(new int[3][4]);
	
		//check all types
	    float a = cast<float>(4);
	    double a = cast<double>(4);
	    code a = cast<code>(4);
	    byte a = cast<byte>(4);
	    boolean a = cast<boolean>(false);     
//	    Wombat a = cast<Wombat>(new Object());  
	    
	    () => () fun = cast<()=>()>blah;        
	    
	    //crazy cast
        (int[][][], int, int ) => ( ()=>(int[][][])) b  = cast<(int[][][], int, int ) => ( ()=>(int[][][]))>(blah);        
   }
}
