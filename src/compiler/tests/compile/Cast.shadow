class Cast {

	public method () => ()
	{
		BaseClass a = new BaseClass();
		a = cast<BaseClass>(new Subclass1()); //typechecks but should fail at run time		
		a = new Subclass2();
		
		int b = cast<int>(5.4);
		// int x = cast<int>(a);  //uncomment to see incomparable types
			
		
		Subclass1 s = cast<Subclass1>(a);		
	}
    
	private class BaseClass
	{}
	
	private class Subclass1 extends Cast.BaseClass
	{}
	
	private class Subclass2 extends BaseClass
	{}

}