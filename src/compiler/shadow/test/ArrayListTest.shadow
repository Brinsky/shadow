import shadow.utility@List;
import shadow.utility@ArrayList;
import shadow.io@Console;

class shadow.test@ArrayListTest
{
	public main( String[] args ) => ( int )
	{
		ArrayList<String> list = ArrayList<String>:create();
		
		try
		{
			for ( int i = 0; i < args->size; i += 1 )
				list.add(check(args[i]));
			for ( int i = 1; i < args->size; i += 3 )
				Console:instance.printLine(list.remove(check(args[i])));
			Console:instance.printLine(list.remove(""));
			for ( int i = 0; i < list->size; i += 1 )
				Console:instance.printLine(list.index(i));
			Console:instance.printLine(list);
		}
		recover
		{}
			
		return 0;
	}
}
