import shadow.io@Console;

class DoubleTest
{
	public main(String[] args) => ()
	{
		double a = 1.0;
		double b = -1.0;
		double c = 1.0;
		double d = -1.0;
		
		Console out = Console:instance;
	
		for( int i = 0; i < 20; i += 1 )
		{
			out.printLine("\nIteration: " # i);
			out.printLine("a: " # a);
			out.printLine("b: " # b);
			out.printLine("c: " # c);
			out.printLine("d: " # d);
			
			a *= 10;
			b *= 10;
			c /= 10;
			d /= 10;
		}
	}
}