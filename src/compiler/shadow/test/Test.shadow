import shadow.standard@Array;
import shadow.io@Console;

class shadow.test@Test
{
	nullable Test test = this;
	public main() => ()
	{
		Console console = instance Console;
		nullable Test test = new Test();
		try {
			console.printLine(check(check(test):test));
		} recover {
			console.printLine(1);
		}
		try {
			check(test):test = null;
		} recover {
			console.printLine(2);
		}
		try {
			console.printLine(check(check(test):test));
		} recover {
			console.printLine(3);
		}
	}
}
