import shadow.standard@Array;
import shadow.io@Console;

class shadow.test@Test
{
	nullable Test test = this;
	public main() => ()
	{
		nullable Test test = Test:create();
		try {
			Console:instance.printLine(check(check(test):test));
		} recover {
			Console:instance.printLine(1);
		}
		Console console = Console:instance;
		try {
			check(test):test = null;
		} recover {
			console.printLine(2);
		}
		try {
			console.printLine(check(check(test):test));
		} recover {
			console.printLine(3);
		}
	}
}
