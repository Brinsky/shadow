import shadow:io@Console;

class AwkwardBreaks
{
	public main() => ()
	{
		Console out;
	
		for( int i = 0; i < 10; )
		{
			out.printLine(i);
			break;			
		}
		
		for( int i = 0; i < 10; i += 1 )
		{
			out.printLine(i);
			continue;			
		}
		
		int[] array = {5, 6, 7, 10};
		foreach( int number in array )
		{
			out.printLine(number);
			break;
		}
		
		foreach( int number in array )
		{
			out.printLine(number);
			continue;
		}
		
		int number = 5;
		
		while( number < 10 )
		{
			out.printLine(number);
			break;		
		}
		
		while( number < 10 )
		{
			out.printLine(number);
			number += 1;
			continue;		
		}		
		
		do
		{
			out.printLine(number);
		} while( false );		
		
		
		int value = 3;
		
		switch(value)
		{
			default out.printLine("Hello!");
			case(3)
			{
				out.printLine("Goodbye!");
				return;
			}
			case(1) out.printLine("Help me!");
		}
	}
}
