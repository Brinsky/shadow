import shadow.utility@List;
import shadow.utility@ArrayList;
import shadow.io@Console;

class shadow.test@ArrayListTest
{
	public main( String[] args ) => ( int )
	{
		ArrayList<String> list = ArrayList<String>:create();
		Console out = Console:instance;
		
		try
		{	
			out.printLine("Adding stuff");		
			for ( int i = 0; i < args->size; i += 1 )
				list.add(check(args[i]));
			out.printLine("Added stuff");
			
			out.printLine("Removing stuff");
			for ( int i = 1; i < args->size; i += 3 )
				out.printLine(list.remove(check(args[i])));
			out.printLine("Removed stuff");
			
			out.printLine(list.remove(""));
			
			for ( int i = 0; i < list->size; i += 1 )
				out.printLine(list.index(i));
			
			out.printLine(list);
		}
		recover
		{
			out.printLine("Recovery needed.");
		}
			
		return 0;
	}
}
