import shadow.io@Console;

class shadow.test@ComplexTest
{
	public main() => ()
	{
		Console out = Console:instance;
		Complex a = Complex:create(2,-1);
		out.printLine("a: " # a);				
		Complex b = Complex:create(3,5);
		out.printLine("b: " # b);
				
		Complex c = a + b;
		out.printLine("c: " # c);
		Complex d = a - b;
		out.printLine("d: " # d);
		Complex e = a * b;
		out.printLine("e: " # e);
	}
}