import shadow:io@Console;

class shadow:test@CommandLine
{
	public main( String[] args ) => ( int )
	{
		String value = "bleh";
		String word = "bleh";
		Console out = Console:instance;
		
		if( value == word )
			out.printLine("Yes!");
		
		try
		{
			for( int i = args->size - 1; i >= 0; i -= 1 )
			{
				value = check(args[i]);
				out.printLine(value);
				
				if( value == word )
					out.printLine("matches!");
				else
					out.printLine("doesn't match!");
			}
		}
		recover {}	
	
		return 0;
	}
}
