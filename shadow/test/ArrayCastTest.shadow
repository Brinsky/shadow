import shadow:io@Console;

class shadow:test@ArrayCastTest
{
	public main() => ()
	{
		Console out;
		
		try
		{
			int[] a = int:create[10];
			Array<int> b = a;
			//explicit cast
			int[] c = cast<int[]>(b);			
			out.printLine("Passed");
		}
		catch(CastException e)
		{
			out.printLine("Failed: " # e);		
		}
		
		try
		{
			int[] a = int:create[10];
			Array<int> b = a;
			//implicit cast
			int[] c = b;
			out.printLine("Passed");
		}
		catch(CastException e)
		{
			out.printLine("Failed: " # e);		
		}		
	}
}