import shadow:io@Console;
import shadow:utility@LinkedList;

class shadow:test@ForeachTest
{
	public main( String[] args ) => ()
	{	
		Console out;
		int[] array = int:create[20];		
		int[,,] values = int:create[2,4,3];
	
		var list = LinkedList<String>:create();
		
		list.add("My");
		list.add("dog");
		list.add("has");
		list.add("fleas");
		
		for( int i = 0; i < array->size; i += 1 )
			array[i] = array->size - i;
			
		int counter = 0;
		for( int i = 0; i < 2; i += 1 )
			for( int j = 0; j < 4; j += 1 )
				for( int k = 0; k < 3; k += 1 )
				{
					values[i,j,k] = counter;
					counter += 1;
				}		
		
		foreach( int value in array )
			out.printLine( value );
			
		foreach( String word in list )
			out.printLine( word );
			
		foreach( int value in values )
			out.printLine( value );
			
		CanIterate<int> iterable = values;
		
		foreach( int value in iterable )
			out.printLine( value );
			
			
		int[][] table = int:create[4][5];
		int start = 20;
		
		for( int i = 0; i < 4; i += 1 )
			for( int j = 0; j < 5; j += 1 ) {
				table[i][j] = start;
				start -= 1;
			}
	
		foreach( int[] row in table ) {
			foreach( int element in row )
				out.print(element # " ");
			out.printLine();
		}			
	}
}
